{
  "name": "local-noise-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run redis:start && npm run build && node dist/index.js",
    "dev": "npm run redis:start && nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "debug": "babel src --out-dir dist --watch --source-maps",
    "redis:start": "redis-server /etc/redis/redis.conf &",
    "redis:flush": "redis-cli FLUSHALL",
    "redis:stop": "redis-cli shutdown &",
    "test": "./node_modules/mocha/bin/mocha",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.15.2",
    "bull": "^2.1.0",
    "date-fns": "^1.21.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "ioredis": "^2.4.0",
    "strcmp": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  }
}
